{
  "name": "@wcfactory/cli",
  "description": "CLI for managing your custom elements at scale.",
  "version": "0.7.4",
  "author": "heyMP @heyMP",
  "bin": {
    "wcf": "./bin/run"
  },
  "bugs": "https://github.com/elmsln/wcfactory/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@wcfactory/common": "^0.7.3",
    "@wcfactory/generator-wcfactory": "^0.7.4",
    "date-fns": "^1.29.0",
    "fuse.js": "^3.4.2",
    "glob": "^7.1.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "inquirer-recursive": "^0.0.6",
    "listr": "^0.14.2",
    "listr-update-renderer": "^0.5.0",
    "listr-verbose-renderer": "^0.6.0",
    "lodash": "^4.17.11",
    "yeoman-environment": "^2.3.3",
    "yeoman-generator": "^4.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@oclif/test": "1.2.5",
    "globby": "11.0.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/src",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/elmsln/wcfactory",
  "keywords": [
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "wcf",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "elmsln/wcfactory",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "prepack": "oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
