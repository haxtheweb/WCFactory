{
  "name": "Slim",
  "description": "data binding +, incredibly small helper functions",
  "wcfactory": {
    "customElementClass": "Slim",
    "templateReturnFunctionPart": "render(tpl) {\n    this._render",
    "propertyBinding": {
      "prefix": "{{",
      "suffix": "}}"
    }
  },
  "main": "Slim/Slim.js",
  "version": "0.0.1",
  "scripts": {
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "dependencies": {
    "slim-js": "^4.0.0"
  },
  "devDependencies": {
    "web-animations-js": "2.3.1",
    "@lrnwebcomponents/deduping-fix": "0.2.0",
    "@polymer/iron-demo-helpers": "3.0.2",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@webcomponents/webcomponentsjs": "2.2.6",
    "concurrently": "4.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.4",
    "lodash": "4.17.11",
    "polymer-cli": "1.9.6",
    "polymer-build": "3.1.1",
    "wct-browser-legacy": "1.0.2"
  }
}
